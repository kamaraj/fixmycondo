{
    "version": 2,
    "env": {
        "VERCEL": "1",
        "DATABASE_URL": "sqlite+aiosqlite:////tmp/fixmycondo.db",
        "UPLOAD_DIR": "/tmp/uploads"
    },
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ]
}